	<insert id="insertOpinion" parameterType="Opinion">
		INSERT INTO opinion (DOC_NO, 	EVLTR_NM1, EVL_CN1, 	EVL_RSLT_CN1,   EVL_CN_CEO,   EVL_RSLT_CN_CEO)
					VALUES(#{doc_no}, #{evltr_nm1},#{evl_cn1},#{evl_rslt_cn1},#{evl_cn_ceo},#{evl_rslt_cn_ceo})
	</insert>
	
	
