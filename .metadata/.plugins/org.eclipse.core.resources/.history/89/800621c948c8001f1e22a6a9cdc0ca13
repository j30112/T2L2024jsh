package com.t2lJsh.t2lJsh.dto;

import java.util.List;



public class Work_perform {
	private List<String> 	se_no;		//글 작성란 구분번호
	private int				doc_no;		//평가서 번호
	private int 			sum_scr_a;	//업무 성과점수
	private List<String> 	ctgry_no;	//카테고리명
	private List<String> 	ajmt_rt;	//달성률
	public List<String> getSe_no() {
		return se_no;
	}
	public void setSe_no(List<String> se_no) {
		this.se_no = se_no;
	}
	public int getDoc_no() {
		return doc_no;
	}
	public void setDoc_no(int doc_no) {
		this.doc_no = doc_no;
	}
	public int getSum_scr_a() {
		return sum_scr_a;
	}
	public void setSum_scr_a(int sum_scr_a) {
		this.sum_scr_a = sum_scr_a;
	}
	public List<String> getCtgry_no() {
		return ctgry_no;
	}
	public void setCtgry_no(List<String> ctgry_no) {
		this.ctgry_no = ctgry_no;
	}
	public List<String> getAjmt_rt() {
		return ajmt_rt;
	}
	public void setAjmt_rt(List<String> ajmt_rt) {
		this.ajmt_rt = ajmt_rt;
	}
	public List<String> getPrfmnc_cn() {
		return prfmnc_cn;
	}
	public void setPrfmnc_cn(List<String> prfmnc_cn) {
		this.prfmnc_cn = prfmnc_cn;
	}
	public String getSe_no_insert() {
		return se_no_insert;
	}
	public void setSe_no_insert(String se_no_insert) {
		this.se_no_insert = se_no_insert;
	}
	public String getCtgry_no_insert() {
		return ctgry_no_insert;
	}
	public void setCtgry_no_insert(String ctgry_no_insert) {
		this.ctgry_no_insert = ctgry_no_insert;
	}
	public String getAjmt_rt_no_insert() {
		return ajmt_rt_no_insert;
	}
	public void setAjmt_rt_no_insert(String ajmt_rt_no_insert) {
		this.ajmt_rt_no_insert = ajmt_rt_no_insert;
	}
	public String getPrfmnc_cn_insert() {
		return prfmnc_cn_insert;
	}
	public void setPrfmnc_cn_insert(String prfmnc_cn_insert) {
		this.prfmnc_cn_insert = prfmnc_cn_insert;
	}
	private List<String> 	prfmnc_cn;	//내용
	
	//버퍼
	private String se_no_insert;		//배열에서 se_no값 가져와 세팅
	private String ctgry_no_insert;		//배열에서 ctgry_no값 가져와 세팅
	private String ajmt_rt_no_insert;	//배열에서 ajmt_rt값 가져와 세팅
	private String prfmnc_cn_insert;	//배열에서 prfmnc_cn값 가져와 세팅
	
	
	
	
	
	
}
